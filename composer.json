{
    "name": "paseto/nfse-legacy",
    "type": "library",
    "description": "API para geração e comunicação da NFSe com as Prefeituras.",
    "keywords": [
        "nfse",
        "sped",
        "nfephp"
    ],
    "homepage": "https://github.com/paseto/nfse-legacy",
    "license": [
        "GPL-3.0+",
        "LGPL-3.0+",
        "MIT"
    ],
    "require": {
        "php": "^7.0",
        "nfephp-org/sped-common": "^5.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.2.3",
        "phpunit/phpunit": "^5.7",
        "scrutinizer/ocular": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "NFePHP\\NFSe\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NFePHP\\NFSe\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.*-dev"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "platform": {
            "php": "7.4.0"
        }
    }
}
